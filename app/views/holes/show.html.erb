<div class="page-header">
  <h3>Hole #<%= @hole.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>Round</dt>
      <dd><%= @hole.round_id %></dd>

      <dt>Number</dt>
      <dd><%= @hole.number %></dd>

      <dt>Par</dt>
      <dd><%= @hole.par %></dd>

      <dt>P1 score</dt>
      <dd><%= @hole.p1_score %></dd>

      <dt>P2 score</dt>
      <dd><%= @hole.p2_score %></dd>

      <dt>P3 score</dt>
      <dd><%= @hole.p3_score %></dd>

      <dt>P4 score</dt>
      <dd><%= @hole.p4_score %></dd>

      <dt>Roll</dt>
      <dd><%= @hole.roll %></dd>

      <dt>Ctp player</dt>
      <dd><%= @hole.ctp_player %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/holes" class="btn btn-primary">
        Back
      </a>
      <a href="/holes/<%= @hole.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_hole/<%= @hole.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
