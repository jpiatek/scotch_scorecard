<div class="page-header">
  <h3>Round: <%= @round.course_name %>, <%= @round.played_on %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Course name</dt>
      <dd><%= @round.course_name %></dd>

      <dt>Played on</dt>
      <dd><%= @round.played_on %></dd>


      <dt>Player 1</dt>
      <dd><%= @round.player_1 %></dd>

      <dt>Player 2</dt>
      <dd><%= @round.player_2 %></dd>

      <dt>Player 3</dt>
      <dd><%= @round.player_3 %></dd>

      <dt>Player 4</dt>
      <dd><%= @round.player_4 %></dd>


      <dt>User</dt>
      <dd><%= @round.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/rounds" class="btn btn-primary">
        Back
      </a>
      <a href="/rounds/<%= @round.id %>/edit" class="btn btn-warning">
        Edit Players
      </a>
      <a href="/delete_round/<%= @round.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
