<div class="page-header">
  <h2>

  <%= @standing %>

    </h2>
  <h5><%= @round.course_name %>, <%= @round.played_on %></h5>
    </div>


    <div class="btn-group btn-group-justified">
      <a href="/rounds/<%= @round.id %>/edit" class="btn btn-success">
        Edit Match Set Up
      </a>
      </div>


<h3>Scores</h3>
<div class="row">
  <div class="col-md-12">
   <table class="table table-striped table-hover">

      <tr>
      <th>Player</th>
      <th>Front</th>
      <th>Back</th>
      <th>Total</th>
      </tr>

      <tr>
      <td><%= @round.player_1 %></td>
      <td><%=@p1_front%></td>
      <td><%=@p1_back%></td>
      <td><%=@p1_total%></td>
      </tr>

      <tr>
      <td><%= @round.player_2 %></td>
