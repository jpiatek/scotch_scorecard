        <td><%=@hole1_team%> </td>
        <td><%=@hole1_man%></td>
            <td>
      <%= select_tag :ctp_player, options_for_select(["", @round.player_1, @round.player_2, @round.player_3, @round.player_4], @round.holes.find_by(:number=>1).ctp_player) %>
        </td>
        <td><div class="checkbox">
        <label for="roll">
          <input type="checkbox" id="roll" name="roll" value="1" <%= "checked" if @round.holes.find_by(:number => 1).roll %>>
        </label>
     </div>
      </td>
        <td><%=@hole1_points%></td>
        <td>
              <button class="btn btn-primary">
              Update Hole
              </button>
     </form>
     <td><td><i class="fa fa-umbrella"></i></td></td>
      </tr>

