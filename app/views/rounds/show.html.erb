          <form action="/update_hole/<%= @round.holes.find_by(:number => 1).id %>" method="post">

      <!-- Hidden input for authenticity token to protect from forgery -->
      <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>">


         <td><input type="hidden" name="number" value="<%= @round.holes.find_by(:number => 1).number %>">
         1</td>
         <input type="hidden" name="round_id" value="<%= @round.holes.find_by(:number => 1).round_id %>">
        <td><input type="text" size="1" id="par" name="par" placeholder="Par" value="<%= @round.holes.find_by(:number => 1).par %>"></td>
        <td><input type="text" size="1" id="p1_score" name="p1_score" placeholder="<%=@round.player_1%>" value="<%= @round.holes.find_by(:number => 1).p1_score %>"></td>
        <td><input type="text" size="1" id="p2_score" name="p2_score" placeholder="<%=@round.player_2%>" value="<%= @round.holes.find_by(:number => 1).p2_score %>"></td>
